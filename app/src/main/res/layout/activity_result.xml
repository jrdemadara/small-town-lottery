<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".features.result.ResultActivity">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingButtonResultBack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="30dp"
        android:clickable="true"
        android:minWidth="48dp"
        android:minHeight="48dp"
        app:backgroundTint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/back"
        app:tint="#FFFFFF"
        tools:ignore="SpeakableTextPresentCheck" />

    <com.google.android.material.appbar.MaterialToolbar
        android:id="@+id/materialToolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:navigationIcon="@drawable/logo_stl" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="30dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/materialToolbar">

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/nexa_bold"
            android:text="Result" />

        <TextView
            android:id="@+id/textViewResultDate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="50dp"
            android:layout_weight="1"
            android:fontFamily="@font/nexa_bold"
            android:text="NOV 13 2022"
            android:textAlignment="textEnd" />

        <ImageView
            android:id="@+id/imageViewResultStatus"
            android:layout_width="77dp"
            android:layout_height="19dp"
            android:layout_weight="1"
            android:scaleType="fitEnd"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

    </LinearLayout>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView2pmResult"
        android:layout_width="370dp"
        android:layout_height="80dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="30dp"
        android:clickable="true"
        app:cardBackgroundColor="#FEF7EE"
        app:cardCornerRadius="5dp"
        app:cardElevation="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout">

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="85dp"
            android:layout_marginTop="30dp"
            android:fontFamily="@font/nexa_xbold"
            android:text="2PM"
            android:textColor="#F1AB55"
            android:textSize="16sp" />

        <TextView

            android:id="@+id/textViewResult2pmResult"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="300dp"
            android:layout_marginTop="25dp"
            android:fontFamily="@font/nexa_xbold"
            android:text="0"
            android:textColor="#F1AB55"
            android:textSize="24sp" />

        <ImageView

            android:layout_width="82dp"
            android:layout_height="wrap_content"
            android:padding="10dp"
            app:srcCompat="@drawable/cardview_1" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView5pmResult"
        android:layout_width="370dp"
        android:layout_height="80dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="30dp"
        android:clickable="true"
        app:cardBackgroundColor="#F9FAFE"
        app:cardCornerRadius="5dp"
        app:cardElevation="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView2pmResult">

        <TextView

            android:id="@+id/textViewDash5pmWinner"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="85dp"
            android:layout_marginTop="30dp"
            android:fontFamily="@font/nexa_xbold"
            android:text="5PM"
            android:textColor="#3D4045"
            android:textSize="16sp" />

        <TextView

            android:id="@+id/textViewResult5pmResult"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="300dp"
            android:layout_marginTop="25dp"
            android:fontFamily="@font/nexa_xbold"
            android:text="0"
            android:textColor="#3D4045"
            android:textSize="24sp" />

        <ImageView

            android:layout_width="82dp"
            android:layout_height="wrap_content"
            android:padding="10dp"
            app:srcCompat="@drawable/cardview_2" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView9pmResult"
        android:layout_width="370dp"
        android:layout_height="80dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="30dp"
        android:clickable="true"
        app:cardBackgroundColor="#F1F7FC"
        app:cardCornerRadius="5dp"
        app:cardElevation="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView5pmResult">

        <TextView
            android:id="@+id/textViewDash9pmWinner"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="85dp"
            android:layout_marginTop="30dp"
            android:fontFamily="@font/nexa_xbold"
            android:text="9PM"
            android:textColor="#348AB1"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/textViewResult9pmResult"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="300dp"
            android:layout_marginTop="25dp"
            android:fontFamily="@font/nexa_xbold"
            android:text="0"
            android:textColor="#348AB1"
            android:textSize="24sp" />

        <ImageView
            android:id="@+id/imageView25"
            android:layout_width="82dp"
            android:layout_height="wrap_content"
            android:padding="10dp"
            app:srcCompat="@drawable/cardview_3" />
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>